<body>
    <title>Admin Sign Up</title>
    <div class="container">
        <h1>Create a New Admin, <%= @admin.name %></h1>
        <%= form_for @new_admin, url: {action: "create"}, class: "form-horizontal" do |f| %>
            <fieldset>
                <%= render 'shared/basic_info', {:f => f, :user => @new_admin} %>
                <div><%= radio_button_tag "type_admin", "ta" %> <b>TA Admin</b></div>
                <div><%= radio_button_tag "type_admin", "enroll" %> <b>Enrollment Admin</b></div>
                <br>
                <div class="actions form-group">
                    <a href="/" class="btn btn-default">Cancel</a>
                    <%= f.submit "Create", class: "btn btn-primary" %>
                </div>
            </fieldset>
        <% end %>
    </div>
</body>